{
    "name": "musa11971/php-jwt-decoder",
    "description": "A lightweight and flexible library to decode JWTs.",
    "keywords": [
        "php-jwt-decoder"
    ],
    "homepage": "https://github.com/musa11971/php-jwt-decoder",
    "license": "MIT",
    "authors": [
        {
            "name": "musa11971",
            "email": "mussesemou99@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "firebase/php-jwt": "^5.2",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "musa11971\\JWTDecoder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "musa11971\\JWTDecoder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
